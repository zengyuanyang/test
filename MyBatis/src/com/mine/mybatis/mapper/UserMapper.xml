<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mine.mybatis.mapper.UserMapper">
	<insert id="insertUser" parameterType="com.mine.mybatis.pojo.User">
		INSERT INTO user (userName,userSex,userAddress,userBirthday) VALUES (#{userName},#{userSex},#{userAddress},#{userBirthday})
	</insert>
	<select id="selectUserById" parameterType="int" resultType="com.mine.mybatis.pojo.User">
		SELECT * FROM user WHERE userId = #{userId}
	</select>
</mapper>